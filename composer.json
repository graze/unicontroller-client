{
    "name": "graze/unicontroller-client",
    "description": "Domino Unicontroller client written in PHP",
    "keywords": [
        "graze",
        "unicontroller-client"
    ],
    "homepage": "https://github.com/graze/unicontroller-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Graze Developers",
            "email": "developers@graze.com",
            "homepage": "http://www.graze.com",
            "role": "Development Team"
        }
    ],
    "require": {
        "php": "^5.5",
        "league/climate": "^3.2",
        "clue/socket-raw": "^1.2"
    },
    "require-dev": {
        "graze/standards": "^1.0",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit" : "5.*",
        "squizlabs/php_codesniffer": "^2.5",
        "symfony/var-dumper": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Graze\\UnicontrollerClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Graze\\UniControllerClient\\Test\\": "tests/src",
            "Graze\\UnicontrollerClient\\Test\\Unit\\": "tests/unit",
            "Graze\\UnicontrollerClient\\Test\\Integration\\": "tests/integration",
            "Graze\\UnicontrollerClient\\ClassGenerator\\": "bin/src"
        }
    }
}
